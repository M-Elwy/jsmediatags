{
  "name": "@bhunter179/jsmediatags",
  "version": "3.9.8",
  "description": "Media Tags Reader (ID3, MP4)",
  "author": {
    "name": "Bhunter"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/jsmediatags.esm.js"
    }
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyam234/jsmediatags.git"
  },
  "keywords": [
    "ID3",
    "tags",
    "mp3",
    "audio",
    "mp4"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/divyam234/jsmediatags/issues"
  },
  "homepage": "https://github.com/divyam234/jsmediatags#readme",
  "optionalPeerDependencies": {
    "buffer": "^5.0.8"
  },
  "devDependencies": {
    "dts-cli": "^2.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "dts build --format esm --transpileOnly",
    "patch": "npm --no-git-tag-version version patch",
    "release": "npm publish --access=public"
  },
  "engines": {
    "node": ">=18"
  }
}
